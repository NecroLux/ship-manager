    "express": "^5.2.1",
    "file-saver": "^2.0.5",
    "googleapis": "^171.4.0",
    "jspdf": "^4.1.0",
    "jspdf-autotable": "^5.0.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/file-saver": "^2.0.5",
